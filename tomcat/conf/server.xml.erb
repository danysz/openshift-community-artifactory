<Server port="-1" shutdown="SHUTDOWN">

    <Service name="Catalina">
        <Connector port="<%= ENV['OPENSHIFT_ARTIFACTORY_PORT']%>" address="<%= ENV['OPENSHIFT_ARTIFACTORY_IP']%>"/>

        <Connector port="<%= ENV['OPENSHIFT_ARTIFACTORY_PORT_AJP']%>" address="<%= ENV['OPENSHIFT_ARTIFACTORY_IP']%>" protocol="AJP/1.3"/>

        <Engine name="Catalina" defaultHost="localhost">
            <Host name="localhost" appBase="webapps"/>
        </Engine>
    </Service>
</Server>

